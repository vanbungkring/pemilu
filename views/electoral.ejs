<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <title>Legislatif</title>
</head>

<body>
  <div class="container" id="app">

    <div class="row">
      <div class="col-md-12">
        <apexchart type=bar height=350 :options="chartOptions" :series="series" />
      </div>
      <div class="col-md-12">
        <table class="table table-sm table-bordered">
          <thead>
            <tr>
              <th scope="col">
                <small><strong>Wilayah</strong></small>
              </th>
              <th scope="col" v-for="key,value in info" v-if="!key.is_aceh">
                <small v-if='!key.is_aceh'><strong>{{key.nama}}</strong></small>
              </th>

            </tr>
          </thead>
          <tbody>
            <tr v-for="dapilKey,dapilValue in dapil">
              <th scope="row"> <small><strong>{{dapilKey.nama}}</strong></small></th>
              <th scope="col" v-for="partaiKey,partaiValue in info" v-if="!partaiKey.is_aceh">
                <small v-if='!partaiKey.is_aceh'>{{hasil.table[dapilValue][partaiValue]}}</small>
              </th>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.10/vue.min.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
  <script src="https://cdn.jsdelivr.net/npm/vue-apexcharts"></script>
  <script src="/javascripts/electoral.js"></script>
</body>

</html>