<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <title>Hello, world!</title>
</head>

<body>
  <h1>Hello, world!</h1>
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <table class="table table-sm table-bordered" id="app">
          <thead>
            <tr>
              <th scope="col">
                <small>Wilayah</small>
              </th>
              <th scope="col" v-for="key,value in info" v-if="!key.is_aceh">
                <small v-if='!key.is_aceh'>{{key.nama}}</small>
              </th>

            </tr>
          </thead>
          <tbody>
            <tr v-for="dapilKey,dapilValue in dapil">
              <th scope="row">{{dapilKey.nama}}</th>
              <th scope="col" v-for="partaiKey,partaiValue in info" v-if="!partaiKey.is_aceh">
                <small v-if='!partaiKey.is_aceh'>{{hasil.table[dapilValue][partaiValue]}}</small>
              </th>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.10/vue.min.js"></script>
  <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script> -->
  <script src="/javascripts/electoral.js"></script>
</body>

</html>
